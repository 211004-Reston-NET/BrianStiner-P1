@{
}
@model IEnumerable<WebInterface.Models.CustomerVM>

<h1> List of Customers</h1>
<table border="1">
<tr>
    <th>ID</th>
    <th>Name</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Address</th>   
 
  
</tr>
    @foreach(var customer in Model){
        <tr>
            <td>@customer.Id</td>
            <td>@customer.Name</td>
            <td>@customer.Phone</td>
            <td>@customer.Email</td> 
            <td>@customer.Address</td>
        </tr>
    }
</table>
